{
    "version": 2,
    "build": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "builds": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  }